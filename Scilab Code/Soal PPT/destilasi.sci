function f =destilasi(T)
    y1=0.47
    y2=0.2
    y3=0.33
    A1=18.304
    B1=3816.4
    C1=-46.13
    A2=16.988
    B2=3599.6
    C2=-26.09
    A3=18.510
    B3=3593.4
    C3=-35.225
    P=1*760 // mmhg
    P1=exp(A1-(B1/(T+C1)))
    P2=exp(A2-(B2/(T+C2)))
    P3=exp(A3-(B3/(T+C3)))
    K1=P1/P
    K2=P2/P
    K3=P3/P
    X1=y1/(P1/P)
    X2=y2/(P2/P)
    X3=y3/(P3/P)
    f= X1+X2+X3-1
endfunction
